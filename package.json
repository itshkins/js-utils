{
  "name": "@itshkins/js-utils",
  "version": "1.1.15",
  "description": "DOM utils",
  "type": "module",
  "main": "./dist/js-utils.cjs.js",
  "exports": {
    ".": "./dist/js-utils-cjs"
  },
  "scripts": {
    "build": "npm run build:iife && npm run build:cjs",
    "build:iife": "esbuild ./src/index.ts --outfile=./dist/js-utils-iife.js --bundle --platform=browser --packages=external --global-name=jsUtils",
    "build:cjs": "esbuild ./src/index.ts --outfile=./dist/js-utils-cjs.js --bundle --platform=node --packages=external",
    "test": "npm run test:unit",
    "test:unit": "jest src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itshkins/js-utils.git"
  },
  "keywords": [
    "Storage",
    "localStorage",
    "sessionStorage",
    "addEventListener",
    "removeEventListener",
    "setTimeout",
    "clearTimeout",
    "setInterval",
    "clearInterval",
    "requestAnimationFrame",
    "cancelAnimationFrame"
  ],
  "author": "itshkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itshkins/js-utils/issues"
  },
  "homepage": "https://github.com/itshkins/js-utils#readme",
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@types/jest": "29.5.2",
    "esbuild": "0.18.10",
    "jest": "29.5.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
